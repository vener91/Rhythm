package  {	import com.nhe.ui.*;		import flash.display.BitmapData;	import flash.display.Sprite;	import flash.events.*;	import flash.geom.Rectangle;
	

	public class Main_IconButtons extends Sprite {		private var _iconButton:IconButton;				private var _multiIconButton:MultiIconButton;				private var _scale9IconButton:Scale9IconButton;					public function Main_IconButtons() {								var img:BitmapData = new SettingsIcon();			_iconButton = new IconButton(img,27,24);			_iconButton.x =_iconButton.y = 30;			addChild(_iconButton);			_iconButton.addEventListener(MouseEvent.CLICK,iconButton_CLICK);						img = new VolumeIcons();			_multiIconButton = new MultiIconButton(img,27,24);			_multiIconButton.x = _iconButton.x+30			_multiIconButton.y  =_iconButton.y			addChild(_multiIconButton);			_multiIconButton.addEventListener(MouseEvent.CLICK, multiIconButton_CLICK);						img = new Scale9Button();			_scale9IconButton = new Scale9IconButton(img,144,45,250,45,new Rectangle(50,10,10,10));			_scale9IconButton.x = 30			_scale9IconButton.y = 90;			addChild(_scale9IconButton);			_scale9IconButton.addEventListener(MouseEvent.CLICK, scale9IconButton_CLICK);		}				public function iconButton_CLICK(e:MouseEvent){			trace("iconButton_CLICK");		}				public function multiIconButton_CLICK(e:MouseEvent){			trace("multiIconButton_CLICK");			_multiIconButton.phase++;		}		public function scale9IconButton_CLICK(e:MouseEvent){			trace("scale9IconButton_CLICK");		}			}	}